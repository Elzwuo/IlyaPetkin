  <DataSets>
    <DataSet Name="DataSet_Ремонты">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Работы1">
            <Value>=Parameters!Работы1.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Ремонт.Датаремонта, Ремонт.Наименованиеработы, Ремонт.СтоимостьРуб, Ремонт.ID_работа
FROM            Ремонт INNER JOIN
                         Работы ON Ремонт.ID_работа = Работы.ID_работа
WHERE        (Ремонт.ID_работа IN (@Работы1))
ORDER BY Ремонт.Датаремонта</CommandText>
      </Query>
